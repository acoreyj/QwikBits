{
  "name": "@genie-design/qwikbits-headless",
  "version": "0.1.0",
  "description": "Headless components for Qwik with a focus on accessibility",
  "publishConfig": {
    "access": "public"
  },
  "main": "./index.qwik.cjs",
  "qwik": "./index.qwik.mjs",
  "module": "./index.qwik.mjs",
  "types": "./qwikbits-headless/src/index.d.ts",
  "type": "module",
  "exports": {
    ".": {
      "import": "./index.qwik.mjs",
      "require": "./index.qwik.cjs"
    }
  },
  "engines": {
    "node": ">=16.0.0"
  },
  "private": false,
  "scripts": {
    "build": "qwik build",
    "build.lib": "vite build --mode lib",
    "build.types": "tsc --emitDeclarationOnly",
    "dev": "vite --mode ssr",
    "dev.debug": "node --inspect-brk ./node_modules/vite/bin/vite.js --mode ssr --force",
    "fmt": "prettier --write .",
    "fmt.check": "prettier --check .",
    "release": "np",
    "start": "vite --open --mode ssr",
    "qwik": "qwik"
  }
}
