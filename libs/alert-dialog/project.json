{
  "name": "alert-dialog",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "libs/alert-dialog/src",
  "projectType": "library",
  "targets": {
    "build": {
      "executor": "@nrwl/vite:build",
      "outputs": ["{options.outputPath}"],
      "options": {
        "outputPath": "dist/libs/alert-dialog",
        "configFile": "libs/alert-dialog/vite.config.ts",
        "mode": "lib"
      }
    },
    "test": {
      "executor": "@nrwl/vite:test",
      "outputs": ["../..//coverage/libs/alert-dialog"],
      "options": {
        "passWithNoTests": true,
        "reportsDirectory": "../../coverage/libs/alert-dialog"
      }
    },
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "outputs": ["{options.outputFile}"],
      "options": {
        "lintFilePatterns": ["libs/alert-dialog/**/*.{ts,tsx,js,jsx}"]
      }
    }
  },
  "tags": []
}
