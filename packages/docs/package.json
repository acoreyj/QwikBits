{
  "name": "my-qwik-docs-starter",
  "description": "Basic start point to build a docs site with Qwik",
  "engines": {
    "node": ">=15.0.0"
  },
  "private": true,
  "scripts": {
    "build": "qwik build",
    "build.client": "pnpm run unocss && vite build",
    "build.preview": "vite build --ssr src/entry.preview.tsx",
    "build.server": "vite build -c adapters/cloudflare-pages/vite.config.ts",
    "build.types": "tsc --incremental --noEmit",
    "deploy": "echo 'Run \"npm run qwik add\" to install a server adapter'",
    "deploy-experimental-local": "wrangler pages dev ./dist --compatibility-date=2023-04-04 --experimental-local",
    "deploy-local": "wrangler pages dev ./dist --local --compatibility-date=2023-04-04",
    "dev": "vite --mode ssr",
    "dev.debug": "node --inspect-brk ./node_modules/vite/bin/vite.js --mode ssr --force",
    "fmt": "prettier --write .",
    "fmt.check": "prettier --check .",
    "lint": "eslint \"src/**/*.ts*\"",
    "preview": "qwik build preview && vite preview --open",
    "start": "vite --open --mode ssr",
    "qwik": "qwik",
    "unocss": "unocss \"./src/**\" -o \"./src/uno.css\""
  },
  "devDependencies": {
    "@builder.io/qwik": "0.105.0",
    "@builder.io/qwik-city": "~0.105.0",
    "@cloudflare/workers-types": "^4.20230404.0",
    "@iconify-json/lucide": "^1.1.80",
    "@julr/unocss-preset-forms": "^0.0.5",
    "@types/eslint": "8.37.0",
    "@types/node": "^18.16.3",
    "@typescript-eslint/eslint-plugin": "5.59.1",
    "@typescript-eslint/parser": "5.59.1",
    "@unocss/cli": "^0.51.5",
    "@unocss/preset-icons": "^0.51.5",
    "@unocss/preset-web-fonts": "^0.51.5",
    "@unocss/preset-wind": "^0.51.5",
    "@unocss/reset": "^0.51.5",
    "@unocss/transformer-directives": "^0.51.5",
    "eslint": "8.39.0",
    "eslint-plugin-qwik": "0.105.0",
    "prettier": "2.8.8",
    "typescript": "5.0.4",
    "undici": "5.22.0",
    "unocss": "^0.51.5",
    "unocss-preset-theme": "^0.6.0",
    "vite": "4.3.3",
    "vite-tsconfig-paths": "4.2.0"
  },
  "dependencies": {
    "@cloudflare/pages-plugin-sentry": "^1.1.1",
    "@qwikbits/headless-ui": "*",
    "@qwikbits/utils": "*",
    "@sentry/browser": "^7.49.0",
    "cal-sans": "^1.0.1",
    "toucan-js": "^3.1.0"
  }
}
