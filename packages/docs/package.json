{
  "name": "my-qwik-docs-starter",
  "description": "Basic start point to build a docs site with Qwik",
  "engines": {
    "node": ">=15.0.0"
  },
  "private": true,
  "scripts": {
    "build": "qwik build",
    "build-storybook": "storybook build",
    "build.client": "vite build",
    "build.preview": "vite build --ssr src/entry.preview.tsx",
    "build.types": "tsc --incremental --noEmit",
    "deploy": "echo 'Run \"npm run qwik add\" to install a server adapter'",
    "dev": "vite --mode ssr",
    "dev.debug": "node --inspect-brk ./node_modules/vite/bin/vite.js --mode ssr --force",
    "fmt": "prettier --write .",
    "fmt.check": "prettier --check .",
    "lint": "eslint \"src/**/*.ts*\"",
    "preview": "qwik build preview && vite preview --open",
    "start": "vite --open --mode ssr",
    "storybook": "storybook dev -p 6006",
    "qwik": "qwik"
  },
  "devDependencies": {
    "@builder.io/qwik": "0.105.0",
    "@builder.io/qwik-city": "~0.105.0",
    "@storybook/addon-essentials": "7.0.7",
    "@storybook/addon-links": "7.0.7",
    "@storybook/blocks": "7.0.7",
    "@storybook/builder-vite": "7.0.7",
    "@storybook/html": "7.0.7",
    "@storybook/html-vite": "7.0.7",
    "@types/eslint": "8.37.0",
    "@types/node": "^18.16.2",
    "@typescript-eslint/eslint-plugin": "5.59.1",
    "@typescript-eslint/parser": "5.59.1",
    "eslint": "8.39.0",
    "eslint-plugin-qwik": "0.105.0",
    "prettier": "2.8.8",
    "storybook": "7.0.7",
    "storybook-framework-qwik": "0.2.0",
    "typescript": "5.0.4",
    "undici": "5.22.0",
    "vite": "4.3.3",
    "vite-tsconfig-paths": "4.2.0"
  },
  "dependencies": {
    "@qwikbits/code-block": "workspace:*",
    "@qwikbits/headless-ui": "workspace:*"
  }
}
